{
  "name": "webpack4",
  "version": "1.0.0",
  "description": "esto sera divertido",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack --entry ./index.js --output ./bundle.js --mode development",
    "build:local": "webpack --mode production",
    "build:external": "webpack --config ./external/webpack.config.js",
    "build:multi": "webpack --config ./multi-entry-points/webpack.config.js",
    "build:css-loader": "webpack --config ./css-loaders/webpack.config.js",
    "build:plugins": "webpack --config ./plugins/webpack.config.js",
    "build:dev": "webpack-dev-server --config ./dev/webpack.config.js",
    "build:babel": "webpack-dev-server --config ./babel/webpack.config.js",
    "build:react": "webpack-dev-server --config ./react/webpack.config.js",
    "build:url": "webpack-dev-server --config ./url-loader/webpack.config.js",
    "build:prepro": "webpack-dev-server --config ./prepro/webpack.config.js",
    "build:duplication": "webpack --config ./duplication/webpack.config.js",
    "build:dll": "webpack --config ./dll/webpack.dll.config.js",
    "build:duplication-fast": "webpack --config ./dll/webpack.config.js",
    "build:dynamic-dll": "webpack --config ./dynamic-imports/webpack.dll.config.js",
    "build:dynamic": "webpack --config ./dynamic-imports/webpack.config.js"
  },
  "keywords": [
    "webpack"
  ],
  "author": "Javier Fuentes Mora",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.8.4",
    "react": "^16.13.0",
    "react-dom": "^16.13.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "babel-loader": "^8.3.0",
    "css-loader": "^6.7.3",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "less": "^3.11.1",
    "less-loader": "^11.1.0",
    "mini-css-extract-plugin": "^2.7.2",
    "node-sass": "^4.13.1",
    "postcss-loader": "^7.0.2",
    "postcss-nested": "^4.2.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^1.3.0",
    "stylus": "^0.54.7",
    "stylus-loader": "^7.1.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^3.10.3"
  }
